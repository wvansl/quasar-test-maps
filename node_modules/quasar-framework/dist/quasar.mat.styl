$breakpoint-sm-max = $breakpoint-sm
$breakpoint-md-min = $sizes[md]
$breakpoint-md-max = $breakpoint-md
$breakpoint-lg-min = $sizes[lg]
$breakpoint-lg-max = $breakpoint-lg
$breakpoint-xl-min = $sizes[xl]
