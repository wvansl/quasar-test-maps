.q-btn-dense
  padding .285em
  min-height 2em
  &.q-btn-round
    padding 0
    height 2.4em
    width 2.4em
  .on-left
    margin-right 6px
  .on-right
    margin-left 6px
.q-btn-fab, .q-btn-fab-mini
